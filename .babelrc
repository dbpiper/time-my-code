{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "maintained node versions"
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "allExtensions": true,
        "isTSX": true,
        "jsxPragma": "React"
      }
    ]
  ],
  "plugins": [
    "@babel/proposal-class-properties"
  ],
  "ignore": [
    "**/__tests__/*"
  ]
}
